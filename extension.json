{
	"name": "StructuredNavigation",
	"version": "1.0.0",
	"author": [
		"Samantha Nguyen"
	],
	"license-name": "MIT",
	"url": "https://www.mediawiki.org/wiki/Extension:StructuredNavigation",
	"descriptionmsg": "structurednavigation-desc",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.32.0",
		"platform": {
			"php": ">= 7.1"
		}
	},
	"callback": "StructuredNavigation\\ExtensionCallback::onRegistrationCallback",
	"MessagesDirs": {
		"StructuredNavigation": [
			"i18n",
			"i18n/api",
			"i18n/permissions",
			"i18n/specials"
		]
	},
	"ExtensionMessagesFiles": {
		"StructuredNavigationNamespaces": "StructuredNavigation.i18n.namespaces.php",
		"StructuredNavigationAliases": "StructuredNavigation.i18n.aliases.php"
	},
	"ServiceWiringFiles": [
		"src/Services/ServiceWiring.php"
	],
	"AutoloadNamespaces": {
		"StructuredNavigation\\": "src/",
		"StructuredNavigation\\Libs\\": "libs/src/"
	},
	"Hooks": {
		"BeforeDisplayNoArticleText": "StructuredNavigation\\Hooks::onBeforeDisplayNoArticleText",
		"ParserFirstCallInit": "StructuredNavigation\\Hooks::onParserFirstCallInit",
		"UserGetReservedNames": "StructuredNavigation\\Hooks::onUserGetReservedNames"
	},
	"SpecialPages": {
		"NavigationSchema": "StructuredNavigation\\Specials\\Wiring::getNavigationSchemaPage",
		"TitlesUsedInNavigation": "StructuredNavigation\\Specials\\Wiring::getTitlesUsedInNavigation"
	},
	"ContentHandlers": {
		"StructuredNavigation": "StructuredNavigation\\Content\\NavigationContentHandler"
	},
	"APIMetaModules": {
		"structurednavigationexamples": {
			"class": "StructuredNavigation\\Api\\ApiMetaNavigationExamples",
			"factory": "StructuredNavigation\\Api\\Wiring::getApiMetaNavigationExamples"
		},
		"structurednavigationschema": {
			"class": "StructuredNavigation\\Api\\ApiMetaNavigationSchema",
			"factory": "StructuredNavigation\\Api\\Wiring::getApiMetaNavigationSchema"
		}
	},
	"APIPropModules": {
		"structurednavigationnavigationdata": {
			"class": "StructuredNavigation\\Api\\ApiQueryNavigationData",
			"factory": "StructuredNavigation\\Api\\Wiring::getApiQueryNavigationData"
		},
		"structurednavigationnavigationhtml": {
			"class": "StructuredNavigation\\Api\\ApiQueryNavigationHtml",
			"factory": "StructuredNavigation\\Api\\Wiring::getApiQueryNavigationHtml"
		},
		"structurednavigationtitlesused": {
			"class": "StructuredNavigation\\Api\\ApiQueryTitlesUsed",
			"factory": "StructuredNavigation\\Api\\Wiring::getApiQueryTitlesUsed"
		}
	},
	"ConfigRegistry": {
		"structurednavigation": "StructuredNavigation\\ExtensionCallback::getGlobalVarConfig"
	},
	"config": {
		"StructuredNavigationReservedUsername": {
			"value": "msg:structurednavigation-reserved-username"
		},
		"StructuredNavigationEnableCopyEmbedFeature": {
			"value": false
		}
	},
	"namespaces": [
		{
			"id": 2940,
			"constant": "NS_NAVIGATION",
			"name": "Navigation",
			"subpages": true,
			"defaultcontentmodel": "StructuredNavigation",
			"protection": [
				"structured-navigation-edit",
				"structured-navigation-create"
			]
		},
		{
			"id": 2941,
			"constant": "NS_NAVIGATION_TALK",
			"name": "Navigation_talk",
			"subpages": true,
			"content": false,
			"defaultcontentmodel": "wikitext"
		}
	],
	"GroupPermissions": {
		"user": {
			"structured-navigation-create": true,
			"structured-navigation-edit": true
		}
	},
	"AvailableRights": [
		"structured-navigation-create",
		"structured-navigation-edit"
	],
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "StructuredNavigation"
	},
	"ResourceModules": {
		"ext.structurednavigation.ui.structurednavigation.styles": {
			"styles": "resources/styles/ui/structurednavigation.less"
		},
		"ext.structurednavigation.ui.structurednavigation.separator.styles": {
			"class": "ResourceLoaderLessVarFileModule",
			"styles": "resources/styles/ui/separator.less",
			"lessMessages": [
				"structurednavigation-separator-symbol"
			]
		},
		"ext.structurednavigation.libs.view.emptystate.styles": {
			"styles": "libs/resources/ooui/EmptyStateView.less"
		},
		"ext.structurednavigation.view.navigationnotfound.styles": {
			"styles": "resources/styles/view/NavigationNotFoundView.less"
		},
		"ext.structurednavigation.content": {
			"scripts": "resources/modules/ext.structurednavigation.content.js",
			"dependencies": [
				"ext.structurednavigation.CopyEmbedWidget"
			]
		},
		"ext.structurednavigation.CopyEmbedWidget": {
			"scripts": "resources/modules/ext.structurednavigation.CopyEmbedWidget.js",
			"styles": "resources/styles/ext.structurednavigation.CopyEmbedWidget.less",
			"messages": [
				"structurednavigation-copy-label",
				"structurednavigation-copy-state-fail",
				"structurednavigation-copy-state-success"
			],
			"dependencies": [
				"mediawiki.widgets",
				"ext.structurednavigation.core"
			]
		},
		"ext.structurednavigation.core": {
			"scripts": "resources/modules/mw.structuredNav.js"
		}
	},
	"CodeMirrorTagModes": {
		"mw-navigation": "text/mediawiki"
	},
	"manifest_version": 2
}
